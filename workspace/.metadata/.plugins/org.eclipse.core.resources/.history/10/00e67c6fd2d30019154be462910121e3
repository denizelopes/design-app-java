package laboratorio1.exercicio2;

import java.util.ArrayList;

public class NotaFiscal {

	public double calculaImpostoTotal (Imposto[] impostos) {
		
		double impostoTotal = 0;
		
		for(var imposto : impostos) {
			impostoTotal += imposto.calculaImposto();
		}
				
		return impostoTotal;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub


        
		Imposto[] impostos = new Imposto[2];
		
		//calculaImpostoTotal (Imposto[] impostos)
		//System.out.println();
		
	}

}
